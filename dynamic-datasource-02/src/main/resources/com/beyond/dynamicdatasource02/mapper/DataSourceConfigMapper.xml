<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.beyond.dynamicdatasource02.mapper.DataSourceConfigMapper">

    <select id="getDataSourceConfig" resultType="com.beyond.dynamicdatasource02.entities.DataSourceConfig">
        select * from data_source where id = #{id}
    </select>

    <select id="listDataSourceConfig" resultType="com.beyond.dynamicdatasource02.entities.DataSourceConfig">
        select * from data_source_config
    </select>
</mapper>
